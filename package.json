{
  "name": "ai-image-generation-mcp-server",
  "version": "1.0.0",
  "description": "AI图片生成MCP服务器 - 智能内容分析与批量图片生成，集成ModelScope FLUX模型",
  "main": "dist/index.js",
  "type": "module",
  "homepage": "https://github.com/ShiXiangYu2/ai-image-generation-mcp-server",
  "repository": {
    "type": "git",
    "url": "https://github.com/ShiXiangYu2/ai-image-generation-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/ShiXiangYu2/ai-image-generation-mcp-server/issues"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "jest",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write src"
  },
  "keywords": [
    "mcp",
    "mcp-server",
    "ai",
    "image-generation",
    "modelscope",
    "flux",
    "web-development",
    "content-analysis",
    "batch-processing"
  ],
  "author": "shivy123",
  "license": "MIT",
  "mcp": {
    "serverType": "stdio",
    "capabilities": {
      "tools": 6,
      "resources": 2
    }
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "tsx": "^4.6.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 